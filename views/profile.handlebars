<div class="dash">
    <div class="title_header">
        <h3>PROFILE</h3>
        <a id="logout" href="/home"><h3>LOGOUT</h3></a>
    </div>
    <div class="dash_body">
        <div id="your_posts">
        <h4>Your posts:</h4>
        </div>
{{#each posts as |post|}}
        <div class="user_posts">
        <h3 id="post_title">{{post.material.type}}</h3>
        <h5 id="post_date">{{format_date post.createdAt}}</h5>
        </div>
{{/each}}
        <button id="postBtn"><h3>POST RECYCLEABLES</h3></button>
    </div>
</div>