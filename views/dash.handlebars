<div id="dash">
  <div class="dash_header">
    <h3>DASHBOARD</h3>
  </div>
  <div class="dash_body">
    <h4>your posts:</h4>
    {{#each posts as |post|}}
      <section class="user_posts">
        <li>{{title}}</li>
        <h5 class="date" id="date">{{date}}</h5>
      </section>
    {{/each}}
    <button id="post"><h3>+ NEW POST</h3></button>
  </div>
</div>